{
  "name": "@yiluxiangbei87110/input-number",
  "version": "1.0.0",
  "description": "",
  "author": "https://github.com/yiluxiangbei87110",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yiluxiangbei87110/input-number.git"
  },
  "keywords": [
    "vue",
    "sfc",
    "component"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/input-number.umd.js",
  "module": "dist/input-number.esm.js",
  "unpkg": "dist/input-number.min.js",
  "browser": {
    "./sfc": "src/input-number.vue"
  },
  "scripts": {
    "dev": "vue-styleguidist server",
    "test": "jest --verbose",
    "doc": "vue-styleguidist build",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/input-number.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/input-number.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/input-number.min.js",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release": "gren release --override"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "element-ui": "^2.7.2",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "minimist": "^1.2.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-styleguidist": "^3.11.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "vue-sfc-cli": "1.10.12",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.(js|md|json)": [
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}
